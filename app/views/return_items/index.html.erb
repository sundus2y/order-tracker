<p id="notice"><%= notice %></p>

<h1>Listing Return Items</h1>

<table>
  <thead>
    <tr>
      <th>Sale item</th>
      <th>Qty</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @return_items.each do |return_item| %>
      <tr>
        <td><%= return_item.sale_item_id %></td>
        <td><%= return_item.qty %></td>
        <td><%= link_to 'Show', return_item %></td>
        <td><%= link_to 'Edit', edit_return_item_path(return_item) %></td>
        <td><%= link_to 'Destroy', return_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Return item', new_return_item_path %>
