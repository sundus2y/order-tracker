<% content_for :header do %>
    <% if @dash_header %>
        <%= render @dash_header %>
    <% else %>
        Dashboard
    <% end %>
<% end %>
<% content_for :content do %>
    <% if @dash_view %>
        <%= render @dash_view %>
    <% else %>
        <%= image_tag('dashboard.png') %>
    <% end %>
<% end %>